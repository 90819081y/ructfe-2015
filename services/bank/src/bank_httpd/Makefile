PROG = bank_httpd
SOURCES = $(PROG).c mongoose.c
CFLAGS = -Os -W -Wall -I../.. $(CFLAGS_EXTRA) -DNS_DISABLE_THREADS -DMONGOOSE_NO_DIRECTORY_LISTING -DMONGOOSE_NO_SSI -DMONGOOSE_NO_AUTH -DMONGOOSE_NO_DAV -DMONGOOSE_NO_WEBSOCKET

all: $(PROG)

$(PROG): $(SOURCES)
	$(CC) $(SOURCES) -o $@ $(CFLAGS)

clean:
	rm -f *.gc* *.dSYM *.o a.out $(PROG)
