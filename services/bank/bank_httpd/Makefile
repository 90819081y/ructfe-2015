PROG = bank_httpd
SOURCES = $(PROG).c mongoose.c
CFLAGS = -Os -W -Wall -I../.. $(CFLAGS_EXTRA) -DMG_DISABLE_RESOLVER -DMG_DISABLE_HTTP_WEBSOCKET -DMG_DISABLE_DAV -DMG_DISABLE_DIRECTORY_LISTING -DMG_DISABLE_JSON_RPC -DMG_DISABLE_MQTT -DMG_DISABLE_DNS

all: $(PROG)

$(PROG): $(SOURCES)
	$(CC) $(SOURCES) -o $@ $(CFLAGS)

clean:
	rm -f *.gc* *.dSYM *.o a.out $(PROG)
