server{
    listen 80 default;
    access_log /home/static/access.log;
    error_log  /home/static/error.log;
    
    root /home/static/;
    index index.html;

    location /static/ {
        try_files $uri @static;
    }

    location @static {
        root /home;
    }

    location = /home {
        rewrite .* http://$main_host/;
    }
}



